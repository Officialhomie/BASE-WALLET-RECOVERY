# Base Mainnet RPC
BASE_RPC_URL=https://mainnet.base.org
# Alternative: https://base-mainnet.g.alchemy.com/v2/YOUR_KEY

# Coinbase Smart Wallet Bundler (ERC-4337)
COINBASE_BUNDLER_URL=https://api.cdp.coinbase.com/v1/bundler

# Coinbase Paymaster (for gas sponsorship)
COINBASE_PAYMASTER_URL=https://api.cdp.coinbase.com/v1/paymaster

# Coinbase Developer Platform API Key (for paymaster/bundler)
CDP_API_KEY=your_cdp_api_key_here

# Smart Wallet Address (the compromised wallet you're recovering)
SMART_WALLET_ADDRESS=0x0000000000000000000000000000000000000000

# Your Safe Owner Address (the one you control, not compromised)
SAFE_OWNER_ADDRESS=0x0000000000000000000000000000000000000000

# Compromised Owner Address (index 1, to be removed)
COMPROMISED_OWNER_ADDRESS=0x0000000000000000000000000000000000000000

# Owner Index to Remove (typically 1)
OWNER_INDEX_TO_REMOVE=1

# Private Key for Signing (your safe owner key - NEVER commit this!)
PRIVATE_KEY=your_private_key_here

# BaseScan API Key (optional, for verification)
BASESCAN_API_KEY=your_basescan_api_key_here
